{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/laboratorio-remoto-service/src",
  "monorepo": true,
  "root": "apps/laboratorio-remoto-service",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/laboratorio-remoto-service/tsconfig.app.json"
  },
  "projects": {
    "laboratorio-remoto-service": {
      "type": "application",
      "root": "apps/laboratorio-remoto-service",
      "entryFile": "main",
      "sourceRoot": "apps/laboratorio-remoto-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/laboratorio-remoto-service/tsconfig.app.json"
      }
    },
    "my-app": {
      "type": "application",
      "root": "apps/my-app",
      "entryFile": "main",
      "sourceRoot": "apps/my-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-app/tsconfig.app.json"
      }
    }
  }
}